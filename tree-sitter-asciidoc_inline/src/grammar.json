{
  "name": "asciidoc_inline",
  "rules": {
    "inline": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "replacement"
        }
      ]
    },
    "replacement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "PATTERN",
          "value": "\\w+"
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
